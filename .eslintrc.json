{
  "extends": [
    "eslint:recommended"
  ],
  "plugins": [
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2019
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "globals": {
    "$": true,
    "ga": true,
    "logger": true,
    "ahoy": true,
    "MustacheUtil": true,
    "extractErrorMessage": true,
    "showErrorMessage": true,
    "Highcharts": true,
    "moment": true,
    "d3": true,
    "Egotter": true,
    "ToastMessage": true,
    "Mustache": true,
    "AsyncLoader": true,
    "Stripe": true
  },
  "rules": {
    "semi": "error"
  }
}
