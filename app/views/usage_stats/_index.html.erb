<div class="menu-items media">
  <div class="media-body">
    <h4 class="h5 media-heading">
      <%= t('searches.usage_stats.title', user: tu.mention_name) %>
      <div class="tooltip-container"><abbr title="<%= t('searches.usage_stats.description', user: tu.mention_name) %>" rel="tooltip">i</abbr></div>
    </h4>

    <div id="<%= graph.object_id %>" class="usage-stats-chart-container"></div>
    <script>
      $(function () {
        var graph_option = $.extend(true, {}, window.usage_stats_column_chart_options);
        graph_option.series[0].data = <%= raw graph[0].to_json %>;
        graph_option.drilldown.series = <%= raw graph[1].to_json %>;
        graph_option.yAxis.visible = false;
        window.lazycall_functions['#<%= graph.object_id %>'] = function () {
          $('#<%= graph.object_id %>').highcharts(graph_option);
        };
      });
    </script>
  </div>
  <div>
    <%= link_to(t('dictionary.view_more'), usage_stats_path(screen_name: tu.screen_name), class: 'btn btn-info btn-block') %>
  </div>
</div>
