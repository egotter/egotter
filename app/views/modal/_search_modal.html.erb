<%= modal_dialog id: 'search-modal', title: t('.title'), size: 'modal-lg' do %>
  <%= render partial: 'shared/search_form', locals: {track_id: 'search-modal', via: current_via('search_modal')} %>
<% end %>

<script>
  $(function () {
    var url = '<%= raw search_histories_path(via: current_via) %>';
    var message = '<%= t('.failed') %>';
    new SearchModal(url, 'search-modal', message);
  });
</script>
